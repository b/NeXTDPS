setrom
/opdef where pop begin		% internaldict begin

/CircleFont 9 dict def
CircleFont begin

/FontType 4 def			% generate Adobe FID
/FontMatrix [1 0 0 1 0 0] def
/FontBBox [-1 -1 1 1] def
/Encoding StandardEncoding def
/CharDefs 6 dict def
/UniqueID 55 def		% unused by any existing Adobe font

CharDefs begin
/circ
   {idtransform
    0 0 1 index 1 sub 1 index 1 sub 1 index 2 add 1 index 2 add
    setcachedevice
    1 0 360 arc fill
   } bind def
/A {0 0 circ} bind def
/B {.5 0 circ} bind def
/C {0 .5 circ} bind def
/D {.5 .5 circ} bind def
/space {0 0 0 0 0 0 setcachedevice} bind def
end % CharDefs

/BuildChar
   {exch begin
    CharDefs begin
    Encoding exch get cvx exec
    end end
   } bind def

/Private 1 dict def		% type 4 font required to have Private dict
Private /UniqueID UniqueID put
Private noaccess pop

end % CircleFont

/CircleFont CircleFont definefont pop	% make it a legitimate font
/CircleFont undefinefont	% get it out of FontDirectory


/InvalidFont 5 dict def
InvalidFont begin

/FontType -1 def		% an invalid font type
/FontMatrix [1 0 0 1 0 0] def
/FontBBox [0 0 0 0] def
/Encoding StandardEncoding def

end % InvalidFont

/InvalidFont InvalidFont definefont pop	% make it a legitimate font
/InvalidFont undefinefont	% get it out of FontDirectory

end % internaldict
setram
