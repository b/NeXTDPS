/vmstats
   {mark
    currentshared {(Shared )} {(Private )} ifelse print
    vmstatus
    exch (used ) print =string cvs print
    ( out of ) print =
    (level = ) print =
    cleartomark
   } def

/cachestats
   {cachestatus
    (MaxBitMapSize ) print =
       {(CachedChars ) (MIDs ) (BitMapStorage )}
       {print
	(used ) print exch =string cvs print
	( out of ) print =
       } forall
   } def
