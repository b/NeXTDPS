% Remove definitions of internaldict operators that are needed during
% VM building but not during product execution

/opdef where pop		% internaldict
dup /opdef undef
dup /systemnamemap undef
dup /definesystemname undef
dup /trickydict undef
dup /registernames undef
dup /registeroperators undef
dup /abort undef
dup /setrom undef
dup /setram undef
pop

% Get rid of =string in the primordial userdict so that it won't be
% propagated into the initial userdict of each context

userdict /=string undef

