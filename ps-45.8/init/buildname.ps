% package for string concatination
% particularly useful for path name compostition

/StartName
  {$SName begin /namestring exch def /ncnt 0 def end}def

/AppendToName
  {$SName begin
   /str exch def
   str type /nametype eq {/str str nmstr cvs def} if
   namestring ncnt str putinterval
   /ncnt ncnt str length add def
   end
  }def

/ReturnName
  {$SName begin namestring 0 ncnt getinterval end}def

/TruncateName
  {$SName begin /tcnt exch def /str exch def
   str length tcnt 2 copy lt {pop}{exch pop}ifelse
   str exch 0 exch getinteval
   end
  }def

/$SName 10 trickydict def
$SName /nmstr 40 string put
